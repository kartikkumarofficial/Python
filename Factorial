#Method 1 by for
num = 5
def factorial1(num):
    n=1
    for i in range(1,num+1):
        n*=i
    return n

print(factorial1(num)) 
factorial1(num)

#Method 2 by recursion

def factorial(num):
    if (num==1) or (num==0):
        return 1
    else:
        return(num*(factorial(num-1)))
print(factorial(num))